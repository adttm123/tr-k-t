print("a - plaskie b - bry≈Çy")
inp = input("a / b ?")
if "a" == inp:
    print('''
    a - pp kwadratu
    b - pp prostokata
    ...
    ''')
elif "b" == inp:
    print('''
    a - pp szescianu
    b - pp prostopadloscianu
    ...
          
    ''')
    inp = input("?")
    if inp == "a":
        a = float(input("a = "))
        print(f"ppSzecianu dla a={a} = {a**2*6}")

else:
    print("Nie ma takiej komendy")
